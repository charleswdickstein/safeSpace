{
   "content_scripts": [ {
      "js": [ "content_script.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_end"
   } ],
   "name": "SafeSpace",
   "description": "Filters offensive terms related to race, gender, sexual orientation to reduce online harrassment",
   "manifest_version": 2,
   "permissions": [ "http://*.google.com/", "http://*.com/", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0"
}